<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <defaultDocument>
            <files>
                <clear />
                <add value="index.php" />
            </files>
        </defaultDocument>
        <handlers>
            <remove name="php" />
            <add name="PHPExecutable" path="*.php" verb="*" modules="FastCgiModule" scriptProcessor="C:\Program Files\php-8.3.28-Win32-vs16-x64\php-cgi.exe" resourceType="Either" requireAccess="Script" />
        </handlers>
    </system.webServer>
</configuration>
